package com.bridgelabs.repository;

import java.util.List;
import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.bridgelabs.dto.DoctorDetailsDto;
import com.bridgelabs.model.DoctorModel;

@Repository
public interface DoctorRepository extends JpaRepository<DoctorModel, Long>
{
	DoctorModel  findByDoctorName(DoctorDetailsDto DoctorDetailsDto);
	
	
	
	List<DoctorModel> findAll();
	

}
